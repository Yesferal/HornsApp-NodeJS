{
  "name": "hornsapp-api",
  "version": "0.0.1",
  "description": "This is an API about concerts at Lima. This API is made with NodeJS, MongoDB  and Typescript.",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --require dotenv/config dist/index.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "debug": "tsc && node --inspect --require dotenv/config dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yesferal/Hornsapp-Api.git"
  },
  "keywords": [],
  "author": "Yesferal Cueva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yesferal/Hornsapp-Api/issues"
  },
  "homepage": "https://github.com/Yesferal/Hornsapp-Api#readme",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.36",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.4",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.10.7"
  }
}
